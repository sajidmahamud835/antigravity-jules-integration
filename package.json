{
    "name": "antigravity-jules-integration",
    "displayName": "Jules for Antigravity",
    "description": "Bridge local Antigravity agents to remote Jules orchestration via MCP.",
    "version": "2.0.0",
    "publisher": "antigravity-platform",
    "license": "MIT",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other",
        "Machine Learning"
    ],
    "keywords": [
        "antigravity",
        "jules",
        "mcp",
        "ai",
        "orchestration"
    ],
    "activationEvents": [
        "onView:julesForAntigravity.panel",
        "workspaceContains:.git"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "jules-sidebar",
                    "title": "Jules Orchestrator",
                    "icon": "resources/jules-icon.svg"
                }
            ]
        },
        "views": {
            "jules-sidebar": [
                {
                    "id": "julesForAntigravity.panel",
                    "name": "Active Sessions",
                    "type": "webview"
                }
            ]
        },
        "commands": [
            {
                "command": "julesForAntigravity.createSession",
                "title": "Jules: Create New Session"
            },
            {
                "command": "julesForAntigravity.refreshSessions",
                "title": "Jules: Refresh Sessions"
            },
            {
                "command": "julesForAntigravity.applyRemoteBranch",
                "title": "Jules: Apply Remote Branch Changes"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pretest": "npm run compile && npm run lint"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^0.6.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.13.0",
        "@typescript-eslint/parser": "^6.13.0",
        "eslint": "^8.54.0",
        "typescript": "^5.3.0"
    }
}